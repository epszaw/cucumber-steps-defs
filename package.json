{
  "name": "cucumber-steps-defs",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.{js,jsx,json}'",
    "lint-staged": "lint-staged",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lamartire/cucumber-steps-defs.git"
  },
  "keywords": [
    "cucumber",
    "cucumber-js",
    "generator",
    "automatization"
  ],
  "author": "lamartire <lamartire@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lamartire/cucumber-stepdefs-generator/issues"
  },
  "homepage": "https://github.com/lamartire/cucumber-steps-defs#readme",
  "module": "src/index.js",
  "bin": "./bin/cucumber-steps-defs",
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "prettier",
      "prettier/standard"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "trailingComma": "all"
        }
      ]
    }
  },
  "eslintIgnore": [
    "/node_modules"
  ],
  "jest": {
    "rootDir": ".",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.6",
    "memfs": "^2.15.4",
    "prettier": "^1.17.1",
    "sharec-essential-config": "^2.0.4-alpha"
  },
  "sharec": {
    "injected": true
  },
  "dependencies": {
    "cucumber": "^5.1.0",
    "gherkin": "^5.1.0",
    "lodash.uniq": "^4.5.0",
    "minimist": "^1.2.0"
  }
}
